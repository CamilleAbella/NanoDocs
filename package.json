{
  "name": "@ghom/nano-docs",
  "author": "ghom",
  "license": "ISC",
  "version": "1.0.0",
  "description": "The official Nano documentation",
  "main": "index.html",
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": false,
    "useTabs": false
  },
  "scripts": {
    "prettier": "prettier src gulpfile.js index.html package.json --write",
    "build": "gulp build",
    "precommit": "npm run build && npm run prettier && git add *",
    "commit": "git commit -m continue",
    "push": "npm run commit && git push origin master"
  },
  "keywords": [
    "discord.js",
    "bot",
    "typescript",
    "handler",
    "docs",
    "documentation"
  ],
  "engines": {
    "node": "v14.3.0",
    "npm": "6.14.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CamilleAbella/NanoDocs.git"
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-markdown-it": "^0.1.1",
    "gulp-sass": "^4.1.0",
    "highlight.js": "^10.2.0",
    "markdown-it": "^11.0.1",
    "prettier": "^2.0.5"
  },
  "dependencies": {}
}
